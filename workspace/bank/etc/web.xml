<?xml version="1.0" encoding="ISO-8859-1" ?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">
  <servlet>
    <servlet-name>log-in</servlet-name>
    <servlet-class>com.bankcore.web.LogIn</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.bankcore.web.Register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>to-deals</servlet-name>
    <servlet-class>com.bankcore.web.ToDeals</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>with-draw</servlet-name>
    <servlet-class>com.bankcore.web.withdraw.Withdraw</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>save-money</servlet-name>
    <servlet-class>com.bankcore.web.save.Save</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>transfer-money</servlet-name>
    <servlet-class>com.bankcore.web.transfer.Transfer</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>to-logs</servlet-name>
    <servlet-class>com.bankcore.web.ToLogs</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>log-out</servlet-name>
    <servlet-class>com.bankcore.web.LogOut</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>log-in</servlet-name>
	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>to-deals</servlet-name>
	<url-pattern>/to</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>with-draw</servlet-name>
	<url-pattern>/withdraw</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>save-money</servlet-name>
	<url-pattern>/save</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>transfer-money</servlet-name>
	<url-pattern>/transfer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>to-logs</servlet-name>
	<url-pattern>/tologs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>log-out</servlet-name>
	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>